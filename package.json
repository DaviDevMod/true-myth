{
  "name": "true-myth",
  "description": "A library for safe functional programming in JavaScript, with first-class support for TypeScript and Flow",
  "main": "dist/index.js",
  "homepage": "https://chriskrycho.github.io/true-myth",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "version": "0.0.1",
  "author": {
    "name": "Chris Krycho",
    "email": "chris@chriskrycho.com",
    "url": "http://www.chriskrycho.com"
  },
  "devDependencies": {
    "@types/jest": "^21.1.0",
    "jest": "^21.1.0",
    "prettier": "^1.7.0",
    "ts-jest": "^21.0.1",
    "typedoc": "^0.8.0",
    "typescript": "2.5"
  },
  "scripts": {
    "test": "jest",
    "doc": "./build-docs",
    "version": "./build-docs && git add docs"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
