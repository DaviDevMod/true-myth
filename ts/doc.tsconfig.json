{
  "extends": "../tsconfig.json",
  "include": ["../src"],
  "compilerOptions": {
    "sourceMap": false,
    "sourceRoot": null
  },
  "typedocOptions": {
    "out": "../docs-dist",
    "excludePrivate": true,
    "excludeInternal": true,
    "includes": "../src",
    "theme": "default",
    "name": "True Myth",
    "entryPointStrategy": "expand",
    "readme": "../README.md",
    "includeVersion": true,
    "githubPages": true
  }
}
